  "scripts": {
    "preinstallcmd": "rm -rf dist",
    "postinstallcmd": "npm run build",
    "build": "npm run build:prod",
    "build:dev": "webpack --env.mode development --open --hot",
    "build:prod": "webpack --env.mode production",
    "start:dev": "webpack-dev-server --env.mode development --open --hot",
    "start:prod": "node server.js",
    "start": "node server.js",
    "webpack": "webpack",
    "lint": "eslint . --ext .js,.jsx && npm run sass-lint",
    "sass-lint": "sass-lint 'src/**/*.scss' -q -v",
    "sass-lint:fix": "sass-lint-auto-fix",
    "lint:fix": "eslint --fix . --ext .js,.jsx && npm run sass-lint",
    "test": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:dev": "jest --coverage --updateSnapshot",
    "test:watch": "jest --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
